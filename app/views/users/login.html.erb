<h1>Login</h1>
<%= form_with url: "/users/sign_in", method: :post do |form| %>
  <div class="field">
    <%= form.label :email %>
    <%= form.text_field :email %>
  </div>
  <div class="field">
    <%= form.label :password %>
    <%= form.text_field :password %>
  </div>
  <div class="actions">
    <%= form.submit %>
  </div>
<% end %>
